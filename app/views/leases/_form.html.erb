<div class="widget">
	<h4>Add New Tenant</h4><hr>
	<%= form_for([obj, obj.leases.new]) do |f| %>
		<div class="form-group">
			<%= f.text_field :start_date, class: 'form-control' %>
		</div>

		<div class="form-group">
			<%= f.text_field :end_date, class: 'form-control' %>
		</div>

		<div class="form-group">
			<%= email_field_tag :email %>
		</div>

		<%= f.submit 'Add Tenant', class: 'btn btn-green btn-block' %>
	<% end %>
</div>